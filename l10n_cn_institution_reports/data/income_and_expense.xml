                <field name="code">CN_INSTITUTIONS_LEX</field>
                <field name="formulas">balance = CN_INSTITUTIONS_5001.balance + CN_INSTITUTIONS_5101.balance + CN_INSTITUTIONS_COS.balance + CN_INSTITUTIONS_5301.balance + CN_INSTITUTIONS_5401.balance + CN_INSTITUTIONS_5501.balance + CN_INSTITUTIONS_5801.balance + CN_INSTITUTIONS_5901.balance</field>

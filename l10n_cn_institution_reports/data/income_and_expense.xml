<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <record id="income_and_expense" model="account.financial.html.report">
        <field name="name">收入费用表(行政事业单位)</field>
        <field name="analytic" eval="True"/>
        <field name="unfold_all_filter" eval="True"/>
        <field name="show_journal_filter" eval="True"/>
        <field name='parent_id' ref='account.account_reports_legal_statements_menu'/>
    </record>

            <record id="account_financial_report_totalincome0_cn_institutions" model="account.financial.html.report.line">
                <field name="name">一、本期收入</field>
                <field name="code">CN_INSTITUTIONS_INC</field>
                <field name="financial_report_id" ref="income_and_expense"/>
                <field name="formulas">balance = CN_INSTITUTIONS_4001.balance + CN_INSTITUTIONS_4101.balance + CN_INSTITUTIONS_4201.balance + CN_INSTITUTIONS_4301.balance + CN_INSTITUTIONS_OPINC.balance + CN_INSTITUTIONS_4601.balance + CN_INSTITUTIONS_4602.balance + CN_INSTITUTIONS_4603.balance + CN_INSTITUTIONS_4604.balance + CN_INSTITUTIONS_4605.balance + CN_INSTITUTIONS_OIN.balance</field>
                <field name="level" eval="0" />
            </record>

                    <record id="account_financial_report_4001_cn_institutions" model="account.financial.html.report.line">
                        <field name="name">（一）财政拨款收入</field>
                        <field name="code">CN_INSTITUTIONS_4001</field>
                        <field name="formulas">balance = -sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_totalincome0_cn_institutions'/>
                        <field name="groupby">account_id</field>
                        <field name="domain" eval="[('account_id.code', '=like', '4001%')]" />
                        <field name="level" eval="1" />
                    </record>

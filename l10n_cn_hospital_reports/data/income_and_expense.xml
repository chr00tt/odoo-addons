<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="income_and_expense" model="account.report">
        <field name="name">收入费用表(医院)</field>
        <field name="root_report_id" ref="account_reports.profit_and_loss"/>
        <field name="filter_analytic_groupby" eval="True"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="filter_journals" eval="True"/>
        <field name="country_id" ref="base.cn"/>
        <field name="filter_multi_company">selector</field>
        <field name="default_opening_date_filter">today</field>
        <field name="column_ids">
            <record id="profit_and_loss_column" model="account.report.column">
                <field name="name">余额</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="account_financial_report_totalincome0_cn_hospital" model="account.report.line">
                <field name="name">一、本期收入</field>
                <field name="code">cn_hospital_INC</field>
                <field name="expression_ids">
                    <record id="account_financial_report_totalincome0_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">cn_hospital_4001.balance + CN_INSTITUTIONS_4101.balance + CN_INSTITUTIONS_4201.balance + CN_INSTITUTIONS_4301.balance + CN_INSTITUTIONS_OPINC.balance + CN_INSTITUTIONS_4601.balance + CN_INSTITUTIONS_4602.balance + CN_INSTITUTIONS_4603.balance + CN_INSTITUTIONS_4604.balance + CN_INSTITUTIONS_4605.balance + CN_INSTITUTIONS_OIN.balance</field>
                        <field name="date_scope">normal</field>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_4001_cn_hospital" model="account.report.line">
                        <field name="name">（一）财政拨款收入</field>
                        <field name="code">cn_hospital_4001</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4001%')])</field>
                    </record>
                    <record id="account_financial_report_4101_cn_hospital" model="account.report.line">
                        <field name="name">（二）事业收入</field>
                        <field name="code">cn_hospital_4101</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4101%')])</field>
                    </record>
                    <record id="account_financial_report_4201_cn_hospital" model="account.report.line">
                        <field name="name">（三）上级补助收入</field>
                        <field name="code">cn_hospital_4201</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4201%')])</field>
                    </record>
                    <record id="account_financial_report_4301_cn_hospital" model="account.report.line">
                        <field name="name">（四）附属单位上缴收入</field>
                        <field name="code">cn_hospital_4301</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4301%')])</field>
                    </record>
                    <record id="account_financial_report_income0_cn_hospital" model="account.report.line">
                        <field name="name">（五）经营收入</field>
                        <field name="code">cn_hospital_OPINC</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_income0_balance_cn_hospital" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'income')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">-sum</field>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_4601_cn_hospital" model="account.report.line">
                        <field name="name">（六）非同级财政拨款收入</field>
                        <field name="code">cn_hospital_4601</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4601%')])</field>
                    </record>
                    <record id="account_financial_report_4602_cn_hospital" model="account.report.line">
                        <field name="name">（七）投资收益</field>
                        <field name="code">cn_hospital_4602</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4602%')])</field>
                    </record>
                    <record id="account_financial_report_4603_cn_hospital" model="account.report.line">
                        <field name="name">（八）捐赠收入</field>
                        <field name="code">cn_hospital_4603</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4603%')])</field>
                    </record>
                    <record id="account_financial_report_4604_cn_hospital" model="account.report.line">
                        <field name="name">（九）利息收入</field>
                        <field name="code">cn_hospital_4604</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4604%')])</field>
                    </record>
                    <record id="account_financial_report_4605_cn_hospital" model="account.report.line">
                        <field name="name">（十）租金收入</field>
                        <field name="code">cn_hospital_4605</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4605%')])</field>
                    </record>
                    <record id="account_financial_report_other_income0_cn_hospital" model="account.report.line">
                        <field name="name">（十一）其他收入</field>
                        <field name="code">cn_hospital_OIN</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">-sum([('account_id.code', '=like', '4609%')])</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_less_expenses0_cn_hospital" model="account.report.line">
                <field name="name">二、本期费用</field>
                <field name="code">cn_hospital_LEX</field>
                <field name="expression_ids">
                    <record id="account_financial_report_less_expenses0_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">cn_hospital_5001.balance + CN_INSTITUTIONS_5101.balance + CN_INSTITUTIONS_COS.balance + CN_INSTITUTIONS_5301.balance + CN_INSTITUTIONS_5401.balance + CN_INSTITUTIONS_5501.balance + CN_INSTITUTIONS_5801.balance + CN_INSTITUTIONS_5901.balance</field>
                        <field name="date_scope">normal</field>
                        <field name="green_on_positive" eval="False"/>
                    </record>
                </field>
                <field name="children_ids">
                    <record id="account_financial_report_5001_cn_hospital" model="account.report.line">
                        <field name="name">（一）业务活动费用</field>
                        <field name="code">cn_hospital_5001</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5001%')])</field>
                    </record>
                    <record id="account_financial_report_5101_cn_hospital" model="account.report.line">
                        <field name="name">（二）单位管理费用</field>
                        <field name="code">cn_hospital_5101</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5101%')])</field>
                    </record>
                    <record id="account_financial_report_cost_sales0_cn_hospital" model="account.report.line">
                        <field name="name">（三）经营费用</field>
                        <field name="code">cn_hospital_COS</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="expression_ids">
                            <record id="account_financial_report_cost_sales0_balance_cn_hospital" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">domain</field>
                                <field name="formula" eval="[('account_id.account_type', '=', 'expense_direct_cost')]"/>
                                <field name="date_scope">normal</field>
                                <field name="subformula">sum</field>
                                <field name="green_on_positive" eval="False"/>
                            </record>
                        </field>
                    </record>
                    <record id="account_financial_report_5301_cn_hospital" model="account.report.line">
                        <field name="name">（四）资产处置费用</field>
                        <field name="code">cn_hospital_5301</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5301%')])</field>
                    </record>
                    <record id="account_financial_report_5401_cn_hospital" model="account.report.line">
                        <field name="name">（五）上缴上级费用</field>
                        <field name="code">cn_hospital_5401</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5401%')])</field>
                    </record>
                    <record id="account_financial_report_5501_cn_hospital" model="account.report.line">
                        <field name="name">（六）对附属单位补助费用</field>
                        <field name="code">cn_hospital_5501</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5501%')])</field>
                    </record>
                    <record id="account_financial_report_5801_cn_hospital" model="account.report.line">
                        <field name="name">（七）所得税费用</field>
                        <field name="code">cn_hospital_5801</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5801%')])</field>
                    </record>
                    <record id="account_financial_report_5901_cn_hospital" model="account.report.line">
                        <field name="name">（八）其他费用</field>
                        <field name="code">cn_hospital_5901</field>
                        <field name="groupby">account_id</field>
                        <field name="foldable" eval="True"/>
                        <field name="domain_formula">sum([('account_id.code', '=like', '5901%')])</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_net_profit0_cn_hospital" model="account.report.line">
                <field name="name">三、本期盈余</field>
                <field name="code">cn_hospital_NEP</field>
                <field name="expression_ids">
                    <record id="account_financial_report_net_profit0_balance_cn_hospital" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">cn_hospital_INC.balance - CN_INSTITUTIONS_LEX.balance</field>
                        <field name="date_scope">normal</field>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>
